VERSION=@PACKAGE_VERSION@.@PKG_RELEASE@
VERSION_DATE="@GIT_COMMIT_DATE@"
PKG_TARGET=@PKG_TARGET@
PKG_TARGET_VERSION=@PKG_TARGET_VERSION@

# Stable plugins artifacts repository url
STABLE_REPO=${CLOUDSMITH_STABLE_REPO:-'@STABLE_REPO@'}

# Scratch builds repository url
UNSTABLE_REPO=${CLOUDSMITH_UNSTABLE_REPO:-'@UNSTABLE_REPO@'}

# Stable manual installation packages repository url
STABLE_PKG_REPO=${CLOUDSMITH_STABLE_PKG_REPO:-'@STABLE_PKG_REPO@'}

# Opencpn api version, part of win32 installer name (major * 100 + minor).
OCPN_API_VERSION=@OCPN_API_VERSION@
